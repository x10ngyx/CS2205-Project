define_expr Square(x) {x * x};
func Area(x) {return Pi()*Square(x)};
define_expr Pi() {3};
proc Plus1(x) {x = x + 1};
define_cmd Minus1(z) {z = z - 1};
func Mul(x,y) {
	if (x < y)  
	then {
		var tmp;
		tmp = x;
		x = y;
		y = tmp
	}
	else {x = x} ;
	var result;
	while(x >= y) 
	do {
		result = result * x;	
		Minus1(x)
	} ;
	return result
}.

var x;
var y;
x = read_int();
if(x < 5)
then{
	y = 2;
	while(Area(x) <= 100) do{x = x + y; Plus1(y)};
	write_int(Area(x))
}
else {
	y = x / 2;
	write_int(Mul(x,y))
}